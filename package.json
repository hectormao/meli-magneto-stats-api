{
    "name": "meli-magneto-stats-api",
    "version": "1.0.0-SNAPSHOT",
    "description": "AWS Lambda that exposes an API to show the mutant checker stats",
    "main": "dist/src/handler.js",
    "types": "dist/src/handler.d.ts",
    "scripts": {
        "build": "tsc -p .",
        "clean": "rm -Rf dist && rm -Rf .nyc_coverage && rm -Rf .nyc_output && rm -Rf *.zip && rm -Rf *.tgz",
        "pack": "npm run clean && npm run build && npm-pack-zip",
        "test": "npm run test:unit && npm run test:integ",
        "test:integ": "ts-mocha --exit -p tsconfig.json './test/integ/*integ.spec.ts'",
        "test:unit": "ts-mocha --exit -p tsconfig.json './test/unit/*unit.spec.ts' './test/unit/**/*unit.spec.ts'",
        "test:coverage": "nyc npm run test:unit",
        "tf:init": "cd terraform && terraform init && cd ..",
        "tf:dev:plan": "cd terraform && terraform plan -var-file=tfvars/dev.tfvars && cd ..",
        "tf:dev:apply": "cd terraform && terraform apply -var-file=tfvars/dev.tfvars && cd ..",
        "run:main": "ts-node ./src/main.ts"
    },
    "dependencies": {
        "aws-sdk": "2.829.0",
        "typescript-logging": "1.0.0",
        "inversify": "5.0.5",
        "reflect-metadata": "0.1.13",
        "http-status": "1.5.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.71",
        "@types/aws-sdk": "2.7.0",
        "@types/chai": "4.2.14",
        "@types/mocha": "8.2.0",
        "@types/node": "14.14.22",
        "@types/http-status": "1.1.2",
        "@types/sinon-chai": "3.2.5",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "aws-sdk-mock": "5.1.0",
        "chai": "4.2.0",
        "chai-as-promised": "7.1.1",
        "eslint": "^7.18.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "http": "0.0.1-security",
        "mocha": "8.2.1",
        "npm-pack-zip": "1.2.7",
        "nyc": "15.1.0",
        "proxyquire": "2.1.3",
        "sinon": "9.2.3",
        "ts-mocha": "8.0.0",
        "ts-mockito": "2.6.1",
        "ts-node": "9.1.1",
        "ts-sinon": "2.0.1",
        "typescript": "4.1.3"
    },
    "bundledDependencies": [
        "aws-sdk",
        "typescript-logging",
        "inversify",
        "reflect-metadata",
        "http-status"
    ],
    "author": "hectormao.gonzalez@gmail.com",
    "license": "Unlicensed"
}
